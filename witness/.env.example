# Base URL for archive files (used when VITE_USE_FIRESTORE=false)
# Local/Vercel static: /archive
VITE_ARCHIVE_BASE_URL=/archive

# Set to true to read archive data from Firestore + Firebase Storage
# instead of static files. Requires migration script to have been run first.
VITE_USE_FIRESTORE=false

# Firebase configuration
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=
VITE_FIREBASE_MEASUREMENT_ID=
